execute if score $level aom.tool matches 1 run function aged:oremagnet/recursion/level/wooden
execute if score $level aom.tool matches 2 run function aged:oremagnet/recursion/level/stone
execute if score $level aom.tool matches 3 run function aged:oremagnet/recursion/level/iron
execute if score $level aom.tool matches 4..5 run function aged:oremagnet/recursion/level/diamond

execute if score $mechanization aom.tool matches 1 run function aged:oremagnet/recursion/level/mechanization